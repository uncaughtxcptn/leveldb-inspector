{
  "name": "leveldb-inspector",
  "version": "1.0.0",
  "description": "A user interface for interacting with LevelDB databases",
  "main": "dist/main-process.js",
  "scripts": {
    "start": "electron .",
    "build": "webpack",
    "watch": "webpack --watch",
    "dist": "electron-builder",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "leveldb"
  ],
  "author": "UncaughtException <uncaughtexception.blog@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "7.8.4",
    "@babel/preset-env": "7.8.4",
    "babel-loader": "8.0.6",
    "css-loader": "3.4.2",
    "devtron": "1.4.0",
    "electron": "8.0.1",
    "electron-builder": "22.7.0",
    "file-loader": "5.1.0",
    "html-webpack-plugin": "3.2.0",
    "husky": "4.2.3",
    "lint-staged": "10.0.7",
    "vue-loader": "15.9.0",
    "vue-style-loader": "4.1.2",
    "vue-template-compiler": "2.6.11",
    "webpack": "4.41.6",
    "webpack-cli": "3.3.11",
    "webpack-node-externals": "1.7.2"
  },
  "dependencies": {
    "core-js": "3.6.5",
    "level": "^6.0.0",
    "lodash": "4.17.15",
    "vue": "2.6.11",
    "vue-router": "3.1.5",
    "vuex": "3.1.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/uncaughtxcptn/leveldb-inspector.git"
  },
  "bugs": {
    "url": "https://github.com/uncaughtxcptn/leveldb-inspector/issues"
  },
  "homepage": "https://github.com/uncaughtxcptn/leveldb-inspector#readme",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,json,vue,html,css}": [
      "prettier --write"
    ]
  },
  "build": {
    "appId": "app.patootie.leveldb-inspector",
    "productName": "LevelDB Inspector",
    "copyright": "Copyright Â© year UncaughtException",
    "directories": {
      "output": "build"
    },
    "files": [
      "dist",
      "package.json",
      "src/images/logo.icns"
    ],
    "mac": {
      "category": "public.app-category.developer-tools",
      "icon": "src/images/logo.icns"
    }
  }
}
