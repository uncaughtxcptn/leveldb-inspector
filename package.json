  "license": "MIT",
  "devDependencies": {
    "@babel/core": "7.10.4",
    "@babel/preset-env": "7.10.4",
    "babel-loader": "8.1.0",
    "css-loader": "3.6.0",
    "devtron": "1.4.0",
    "electron": "9.1.0",
    "electron-builder": "22.7.0",
    "file-loader": "6.0.0",
    "html-webpack-plugin": "4.3.0",
    "husky": "4.2.5",
    "lint-staged": "10.2.11",
    "vue-loader": "15.9.3",
    "vue-style-loader": "4.1.2",
    "vue-template-compiler": "2.6.11",
    "webpack": "4.43.0",
    "webpack-cli": "3.3.12",
    "webpack-node-externals": "1.7.2"
  },
  "dependencies": {
    "core-js": "3.6.5",
    "electron-updater": "4.3.1",
    "level": "6.0.1",
    "lodash": "4.17.15",
    "vue": "2.6.11",
    "vue-router": "3.3.4",
    "vuex": "3.5.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/uncaughtxcptn/leveldb-inspector.git"
  },
  "bugs": {
    "url": "https://github.com/uncaughtxcptn/leveldb-inspector/issues"
  },
  "homepage": "https://github.com/uncaughtxcptn/leveldb-inspector#readme",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,json,vue,html,css}": [
      "prettier --write"
    ]
  }
